{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "X-Requested-With, Content-Type, Accept" },
        { "key": "Content-Security-Policy", "value": "default-src * 'unsafe-inline' 'unsafe-eval' data: blob:; connect-src * 'unsafe-inline'; font-src * data:; frame-src *; img-src * data: blob:; media-src * blob:; object-src *; script-src * 'unsafe-inline' 'unsafe-eval'; style-src * 'unsafe-inline';" }
      ]
    }
  ],
  "env": {
    "REACT_APP_GOOGLE_CLIENT_ID": "1033835493377-4gk2e80bhgsr51j6f9j9vo5mp7k7j1ad.apps.googleusercontent.com",
    "REACT_APP_GOOGLE_API_KEY": "AIzaSyBVxLt7aRqNmDRL6NLq2CRSVKrPb3IeYIw",
    "REACT_APP_ROOT_FOLDER_ID": "1MTFQM7oGUGDg5xYwbuuw7rwrXXfoU-a9"
  },
  "buildCommand": "chmod +x vercel-build.sh && ./vercel-build.sh",
  "installCommand": "npm install",
  "build": {
    "env": {
      "CI": "false"
    }
  }
} 